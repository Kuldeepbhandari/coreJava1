class MyThread extends Thread
{
	String s="DarkkNight";
	public void run(){
		synchronized (this) {
		for(int i=0;i<s.length();i++){
			try {
				sleep(1000);
			} catch (InterruptedException e) {
				// TODO Auto-generated catch block
				e.printStackTrace();
			}
			System.out.println(Thread.currentThread().setName(s.charAt(i));
			
	
	}}
	}
}

