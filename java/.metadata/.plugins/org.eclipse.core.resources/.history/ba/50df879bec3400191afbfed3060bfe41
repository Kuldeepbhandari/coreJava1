package com.anr.util;

public  abstract class MathUtil {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		public static boolean isPrime(int posInt){
			boolean b=true;
			for(int j=2;j<posInt;j++){
				if(posInt%j==0){
					b=false;
					break;
				}
			}
			return b;
		}
	}

}
