import java.util.Scanner;


public class PowerOf2Triangle {

	public static void pattern()
	{
		Scanner s=new Scanner(System.in);
		System.out.println("Enter The Value");
		int num=s.nextInt();
		for(int i=1;i<=num;i++)
		{
			for(int j=num;j>i;j--)
			{
				System.out.print(" ");
			}
			for(int k=1;k<=2*num-1;k++)
			{   //if(i==3 && )
				System.out.print(k);
			}
			System.out.println("");
		}
	}
}
