import java.util.Scanner;


public class PrintPatternK {
	public static void Pattern()
	{
		Scanner s=new Scanner(System.in);
		System.out.println("Enter The Value");
		int num=s.nextInt();
		for(int i=1;i<=num;i++)
		{
			/*for(int j=1;j<=i;j++)
			{
				System.out.print(" "+j);
			}*/
		//	System.out.println("");
		
			
		/*for(int k=1;k<num;k++)
		{*/
			for(int k=(num-1)*2 -1;k>=i;k--){
			System.out.print(" ");
			}
			for(int m=i;m>=1;m--)
			{
				System.out.print(m);
			}
			System.out.println("");
		}
		}
		
		
	}

