import java.util.Scanner;


public class PrintPatternJ {

	public static void Pattern()
	{
		Scanner s=new Scanner(System.in);
		System.out.println("Enter The Value");
		int num=s.nextInt();
		for(int i=num;i>=1;i--)
		{
			for(int j=i;j>num;j++)
			{
					System.out.print(" ");
				}	
			for(int k=1;k<=i;k++)
			{
				System.out.print(k);
			}
			}
			System.out.println("");
		}	
		}


