
public class charAtEx {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		String s="hello how are u";
		int counter=0;
		for(int i=0;i<s.length();i++){
			if(s.charAt(i)=='h')
			{
				counter++;
			}
			System.out.println(s.charAt(i) +"occurance of h is  "+counter );
			break;
		}
	}

}
