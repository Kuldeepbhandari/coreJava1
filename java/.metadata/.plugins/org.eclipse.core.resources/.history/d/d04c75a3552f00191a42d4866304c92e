import java.util.Scanner;


public class PrintPatternsA {
public static void Pattern()
{
	Scanner s=new Scanner(System.in);
	System.out.println("Enter The Value");
	int num=s.nextInt();
	for(int i=0;i<num;i++)
	{
		for(int j=0;j<=i;j++)
		{
			System.out.println(j);
			j++;
		}
	}
	System.out.println("");
}
}
