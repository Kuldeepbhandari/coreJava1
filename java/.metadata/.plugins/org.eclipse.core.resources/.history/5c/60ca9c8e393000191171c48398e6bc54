import java.util.Scanner;


public class PrintPatternI {
public static void Pattern()
{
	Scanner s=new Scanner(System.in);
	System.out.println("Enter The Value");
	int num=s.nextInt();
	for(int i=1;i<=num;i++)
	{
		for(int k=i;k<num;k++)
		{
			System.out.print(" ");
			
		}
		for(int j=1;j<=2*num-1;j++)
		{ if(i%10 ==i)
			System.out.print(i);
		}
		System.out.println("");
	}
}
}
