import java.util.Scanner;


public class PrintPatternL {
public static void Pattern()
{
	Scanner s=new Scanner(System.in);
	System.out.println("Enter The Value");
	int num=s.nextInt();
	for(int i=1;i<=num;i++)
	{
		for(int k=num;k>1;k--)
		{
			System.out.print(" ");
			
		}
		for(int j=i;j<=2*i-1;j++)
		{if(j%10)
			{
			
			System.out.print("");
		}else
		{			
			System.out.print(j);
		}}
		System.out.println("");
	}
}
}
