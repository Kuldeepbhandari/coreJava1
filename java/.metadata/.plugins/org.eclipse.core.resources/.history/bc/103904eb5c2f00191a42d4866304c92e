import java.util.Scanner;


public class PrintPatternF {
	public static void pattern()
	{
	Scanner s=new Scanner(System.in);
	System.out.println("Enter The Value");
	int num=s.nextInt();
	for(int i=1;i<=num;i++)
	{
		
		for(int j=1;j<i;j++)
		{
			System.out.print(" ");
		}
		for(int k=1;k<=2*num-i;k++)
		{
			System.out.print("#");
		}
		//for(int l=)
		System.out.println("");
	}
	}
}
