import java.util.Scanner;


public class PrintPatternL {
public static void Pattern()
{
	Scanner s=new Scanner(System.in);
	System.out.println("Enter The Value");
	int num=s.nextInt();
	
	for(int i=1;i<=num;i--)
	{
		for(int j=num;j>i;i--)
		{
			System.out.print(" ");
		}
		for(int k=i ;k<=num;k++)
		{
			System.out.print(k);
		}
		/*{if((i+k)<=num+1)
		{
			System.out.print(k+"\t");
		}
		else
		{
			System.out.print("\t");
		}
		}*/
			
			
			System.out.println("");
		}
		
	
	}
}

