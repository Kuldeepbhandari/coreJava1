import com.anr.threadexa.ThreadSynchronization;


public class ThreadSynchronizationMain {

	
	public static void main(String[] args) {
		int counter=0;
		Runnable run=new ThreadSynchronization();
		Thread t=new Thread(run,"Thred 1");
		Thread t1=new Thread(run,"Thred 2");
		Thread t2=new Thread(run,"Thred 3");
		Thread t3=new Thread(run,"Thred 4");
		Thread t4=new Thread(run,"Thred 5");
		Thread t5=new Thread(run,"Thred 6");
		Thread t6=new Thread(run,"Thred 7");
		Thread t7=new Thread(run,"Thred 8");
		Thread t8=new Thread(run,"Thred 9");
		Thread t9=new Thread(run,"Thred 10");
		t.start();
		if(Thread.currentThread().isAlive())
		{
			counter++
		}
		t1.start();
		t2.start();
		t3.start();
		t4.start();
		t5.start();
		t6.start();
		t7.start();
		t8.start();
		t9.start();
		
	}
}
