import java.util.Scanner;


public class PrintPatternI {
public static void Pattern()
{
	Scanner s=new Scanner(System.in);
	System.out.println("Enter The Value");
	int num=s.nextInt();
	for(int i=1;i<=num;i++)
	{
		for(int k=num;k>1;k--)
		{
			System.out.print(" ");
			
		}
		for(int j=i;j<=2*i-1;j++)
		{ if(i%10 ==i)
			System.out.print(i);
		}
		System.out.println("");
	}
}
}
