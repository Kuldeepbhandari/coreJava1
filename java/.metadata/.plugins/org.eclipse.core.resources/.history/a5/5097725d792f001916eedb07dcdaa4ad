import java.util.Scanner;


public class PrintPatternJ {

	public static void Pattern()
	{
		Scanner s=new Scanner(System.in);
		System.out.println("Enter The Value");
		int num=s.nextInt();
		for(int i=1;i<=num;i++)
		{
			for(int j=1;j<i;j++)
			{
				System.out.print(" ");
			}
			for(int k=1;k<=num;k++)
			{
				System.out.print(k);
				k--;
				/*if(j>=i && j<=2*num-i)
				{
					System.out.print(j);
				}
				else
				{
					System.out.print(" ");
				}*/
			}
			System.out.println("");
		}
	}
}
