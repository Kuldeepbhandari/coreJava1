import java.util.Scanner;


public class PrintPatternG {
public static void pattern()
{
	Scanner s=new Scanner(System.in);
	System.out.println("Enter The Value");
	int num=s.nextInt();
	for(int i=1;i<=num;i++)
	{ int mid=num/2;
		for(int j=1;j<=num;j++)
		{
			if( i==num|| i==1 ||i==mid+1 ||(j<=mid && j<mid+i)  )
			{
				System.out.print("#");
			}
			else
			{
				System.out.print(" ");
			}
		}
		System.out.println(" ");
	}
	
}
}
