import java.util.Scanner;


public class PrintPatternJ {
public static void Pattern()
{
	Scanner s=new Scanner(System.in);
	System.out.println("Enter The Value");
	int num=s.nextInt();
	for(int i=1;i<=num;i++)
	{
		for(int j=num;j>=1;j--)
		{
			System.out.print(j);
		}
		System.out.println("");
	}
}
}
