package com.anr.threadexa;

public class ForEven implements Runnable{

	@Override
	public void run() {
		// TODO Auto-generated method stub
		isEven();
		
	}

	private boolean isEven() {
		for(int i=1;i<10;i++){
			if(i%2==0){
				System.out.println("The even no is " +i);
			}
		}
		return true;		
	}

}
