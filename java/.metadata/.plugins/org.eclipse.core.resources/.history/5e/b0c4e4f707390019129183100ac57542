package com.anr.threadexa;

public class ForOdd  implements Runnable{

	@Override
	public void run() {
		// TODO Auto-generated method stub
		isOdd();
		
	}

	private boolean isOdd() {
		for(int i=1;i<10;i++)
		{
			if(i%2==1)
			{
				System.out.println("The odd no is "+i);
			}
		}
		return false;		
	}

}
