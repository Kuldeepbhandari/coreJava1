
public class MyThread extends Thread {

	public void run(){
		String s="DarkkNight";
		synchronized (this) {
		for(int i=0;i<s.length();i++){
			try {
				sleep(1500);
			} catch (InterruptedException e) {
				// TODO Auto-generated catch block
				e.printStackTrace();
			}
			System.out.print(s.charAt(i)+" ");
			System.out.println(Thread.currentThread().getName());
}
}}
}