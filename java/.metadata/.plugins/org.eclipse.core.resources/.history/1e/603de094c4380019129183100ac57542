
public class ThreadOddEven  implements Runnable{

	@Override
	public void run() {
		 isOdd();
		 isEven();
	}

	private boolean isEven() {
		// TODO Auto-generated method stub
		
		for(int i=1;i<10;i++)
		{
			if(i%2==0)
			{
				System.out.println("Number is even"+i);
			}
		}
		return true;
		
	}

	private boolean isOdd() {
		// TODO Auto-generated method stub
		for(int i=1;i<10;i++){
			System.out.println("Number is odd"+i);
		}
		return true;
	}
	

}
