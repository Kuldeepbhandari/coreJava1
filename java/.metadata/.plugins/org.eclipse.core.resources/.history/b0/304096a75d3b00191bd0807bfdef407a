package com.anr.util;

import com.anr.entity.Student;

public class StudentUtil {
Student stu;
	
	public static void sort(Student[]id)
	{
		int temp=0;
		int a=0;
		int b=0;
		for(int i=0;i<id.length;i++){
			for(int j=i+1;j<id.length;j++){
		if(id[i].getId()<id[j].getId())
		{
			temp=id[i].getId();
			a=id[j].getId();
			b=temp;
			
		}
			
	}
}
}
}