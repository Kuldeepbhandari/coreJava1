public class WritingTheFile {

	public static void writeFile(String []v,String fileName) throws IOException
	{
		FileWriter f=new FileWriter("gfd.txt");
		PrintWriter p=new PrintWriter(f);
	for(int i=0;i<v.length;i++)
	{
		System.out.println((v[i]));
	}
	}
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		WritingTheFile wtf=new WritingTheFile();
		wtf.writeFile("kdjkdjk", "fileName.txt");
	}

}
