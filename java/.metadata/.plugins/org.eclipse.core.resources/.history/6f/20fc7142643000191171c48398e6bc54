import java.util.Scanner;


public class PrintPatternL {
public static void Pattern()
{
	Scanner s=new Scanner(System.in);
	System.out.println("Enter The Value");
	int num=s.nextInt();
	
	for(int i=0;i<=num;i++)
	{
		for(int k=num;k>0;k--)
		{if((i+k)<=num)
		{
			System.out.print(k+"\t");
		}
		else
		{
			System.out.print("\t");
		}
		}
			/*for(int j=i;j<=2*i-1;j++)
				
			{			j=j%10;
				System.out.print(j);	
			}
			if(j>10)*/
			
			System.out.println("");
		}
		
	
	}
}

