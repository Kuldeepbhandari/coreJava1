import java.util.Scanner;


public class PrintPatternF {
	public static void pattern()
	{
	Scanner s=new Scanner(System.in);
	System.out.println("Enter The Value");
	int num=s.nextInt();
	for(int i=1;i<=num;i++)
	{
		
		for(int j=1;j<=num*2-1;j++)
		{
			System.out.print("#");
		}
		for(int k=0;k<i;k++)
		{
			System.out.print(" ");
		}
		System.out.println("");
	}
	}
}
