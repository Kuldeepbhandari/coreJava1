import java.util.Scanner;


public class PrintPatternC {
	public static void Pattern()
	{
		Scanner s=new Scanner(System.in);
		System.out.println("Enter the value");
		int num=s.nextInt();
		for(int i=1;i<=num;i++)
		{
			for(int j=1;j<=num;j++)
			{
				if(i==1&& i==num&& i==(num/2)+1)))
				{
					
				}
			}
		}
	}

}
