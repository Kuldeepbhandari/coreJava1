import java.util.Scanner;


public class PrintPatternG {
public static void pattern()
{
	Scanner s=new Scanner(System.in);
	System.out.println("Enter The Value");
	int num=s.nextInt();
	for(int i=1;i<=num;i++)
	{
		for(int j=1;j<=num;j++)
		{
			if( i==num|| i==1 ||j==num/2+1)
			{
				System.out.print("#");
			}
			else
			{
				System.out.print(" ");
			}
		}
		System.out.println(" ");
	}
	
}
}
