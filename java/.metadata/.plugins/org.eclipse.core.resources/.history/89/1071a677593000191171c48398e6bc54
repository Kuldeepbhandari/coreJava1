import java.util.Scanner;


public class PrintAnimalPattern {
public static void pattern()
{
	Scanner s=new Scanner(System.in);
	System.out.println("Enter a String");
	String num=s.next();
	int i=0;
	int len=num.length();
	for (i=1;i<=len;i++)
	{ System.out.println(num.charAt(i));
	}
		for(int j=1;j<=len;j++)
		{
			if((i==1 &&  j==len)||(i==2 && j==1))
			{
				System.out.print("*");
			}
		}
		System.out.println("");
	}
	
}

