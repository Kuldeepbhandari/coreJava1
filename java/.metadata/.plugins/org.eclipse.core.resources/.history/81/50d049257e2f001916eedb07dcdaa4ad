import java.util.Scanner;


public class PrintPatternK {
	public static void Pattern()
	{
		Scanner s=new Scanner(System.in);
		System.out.println("Enter The Value");
		int num=s.nextInt();
		/*for(int i=1;i<=num;i++)
		{
			for(int j=1;j<=i;j++)
			{
				System.out.print(j);
			}	
		}
		System.out.println("");*/
		for(int k=1;k<num;k++)
		{
			for(int l=num;l>1;l--)
			{
				System.out.print(" ");
			}
			for(int m=k;m>=1;m--)
			{
				System.out.print(m);
			}
			System.out.println("");
		}
		
	}
}
