import java.util.Scanner;


public class PrintPatternL {
public static void Pattern()
{
	Scanner s=new Scanner(System.in);
	System.out.println("Enter The Value");
	int num=s.nextInt();
	int a=num;
	for(int i=1;i<=num;i++)
	{
		for(int k=a;k>i;k--)
		{
			System.out.print(" ");
			
		}
			/*for(int j=i;j<=2*i-1;j++)
				
			{			j=j%10;
				System.out.print(j);	
			}
			if(j>10)*/
			for(int j=0;j<num;j++)
			{
				System.out.print(j);
			}
			System.out.println("");
		}
		
	
	}
}

