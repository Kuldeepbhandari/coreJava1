import java.util.Scanner;


public class PrintPatternL {
public static void Pattern()
{
	Scanner s=new Scanner(System.in);
	System.out.println("Enter The Value");
	int num=s.nextInt();
	for(int i=1;i<=num;i++)
	{
		/*for(int k=num;k>1;k--)
		{
			System.out.print(" ");
			
		}*/
		if(i>5)
		{
			for(int j=0;j<num/2;j++)
			{
				System.out.print(j);
			}
		}else
		{
		for(int j=i;j<=2*i-1;j++)
		
		{			
			System.out.print(j);
		}
		
		System.out.println("");
	}
	}
}
}
